<th>
  <h3>
    {{chore.name}}
    <small>(${{chore.rate}})</small>
  </h3>
</th>

{{#each choreDays as |choreDay|}}
  <td class="chore">
    {{#if choreDay.complete}}
      <button {{action "uncomplete" choreDay}}>
        <i class="fa fa-check"></i>
      </button>
    {{else}}
      <button {{action "complete" choreDay}}/>
    {{/if}}
  </td>
{{/each}}
